{
  "name": "github-browser",
  "productName": "Github Browser",
  "description": "Github browser.",
  "version": "1.5.2",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tborychowski/github-browser.git"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron index.js",
    "pack": "build --dir",
    "build": "build --mac",
    "release": "build --mac --publish always"
  },
  "author": "Tom",
  "license": "MIT",
  "build": {
    "appId": "com.electron.github-browser",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "publish": [
      "github"
    ]
  },
  "dependencies": {
    "electron-config": "^0.2.1",
    "electron-dl": "^1.9.0",
    "electron-log": "^2.0.0",
    "electron-updater": "^1.12.0",
    "electron-window-state": "^4.1.0",
    "ionicons": "^3.0.0",
    "jenkins-api": "^0.2.8",
    "lodash.get": "^4.4.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "tingodb": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "electron": "^1.7.0",
    "electron-builder": "^17.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-run-electron": "^3.0.1",
    "gulp-stylus": "^2.6.0"
  }
