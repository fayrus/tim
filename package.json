{
  "name": "github-browser",
  "productName": "Github Browser",
  "description": "Github browser.",
  "version": "0.20.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tborychowski/github-browser.git"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron index.js",
    "pack": "build --mac",
    "publish": "build --publish always"
  },
  "author": "Tom",
  "license": "MIT",
  "build": {
    "appId": "com.electron.github-browser",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "publish": [
      "github"
    ]
  },
  "dependencies": {
    "electron-config": "^0.2.1",
    "electron-is-dev": "^0.1.2",
    "electron-log": "^1.3.0",
    "electron-updater": "^1.8.1",
    "electron-window-state": "^4.0.1",
    "ionicons": "^3.0.0",
    "jenkins-api": "^0.2.8",
    "octonode": "^0.7.10",
    "tingodb": "^0.5.1"
  },
  "devDependencies": {
    "electron": "beta",
    "electron-builder": "^15.1.1",
    "electron-packager": "^8.3.0"
  }
}
