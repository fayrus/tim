{
  "name": "github-browser",
  "productName": "Github Browser",
  "description": "Github browser.",
  "version": "1.3.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tborychowski/github-browser.git"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron index.js",
    "pack": "build --dir",
    "release": "build --mac --publish always"
  },
  "author": "Tom",
  "license": "MIT",
  "build": {
    "appId": "com.electron.github-browser",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "publish": [
      "github"
    ]
  },
  "dependencies": {
    "electron-config": "^0.2.1",
    "electron-log": "^2.0.0",
    "electron-updater": "^1.12.0",
    "electron-window-state": "^4.1.0",
    "ionicons": "^3.0.0",
    "jenkins-api": "^0.2.8",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "tingodb": "^0.5.1"
  },
  "devDependencies": {
    "electron": "beta",
    "electron-builder": "^16.8.3"
  }
}
